<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.kw.pgb-oauth" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PGB Oauth</name>
    <description>
        A sample Apache Cordova application that comms with a remote site to allow oauth.
    </description>
    <content src="index.html" />
    <access origin="*" />
    
    <feature name="InAppBrowser">
      	<param name="android-package" value="org.apache.cordova.InAppBrowser" />
      	<param name="ios-package" value="CDVInAppBrowser" />
  	</feature>
  
</widget>
